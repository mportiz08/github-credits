#header.rounded
  %h1#title
    = @project.name
  %p.description
    = @project.description
%ul
  - @contributors.each do |c|
    %li.rounded
      .avatar
        %img{:src => c.avatar}
      .user-info
        %h1
          = c.name
        %p
          = c.location
        %p
          = "#{c.commits} commits"
        %p &nbsp;
        %p
          = c.url
        %p
          = c.company
      .clear
